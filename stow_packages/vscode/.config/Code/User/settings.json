    "flake8.enabled": false,
    "git-graph.maxDepthOfRepoSearch": 3,
    "git-graph.repository.commits.loadMore": 300,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.detectSubmodulesLimit": 100,
    "git.fetchOnPull": true,
    "git.openRepositoryInParentFolders": "never",
    "git.repositoryScanMaxDepth": 3, // Controls the depth used when scanning workspace folders for Git repositories when `git.autoRepositoryDetection` is set to true or subFolders. Can be set to -1 for no limit.
