    "diffEditor.diffAlgorithm": "advanced",
    "editor.bracketPairColorization.enabled": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.detectIndentation": false,
    "editor.fastScrollSensitivity": 9,
    "editor.fontFamily": "'Atkinson Hyperlegible Mono', 'Source Code Pro','Droid Sans Mono', 'monospace', monospace, Menlo, Monaco, 'Courier New'",
    "editor.fontLigatures": true,
    "editor.guides.bracketPairs": "active",
    "editor.largeFileOptimizations": false,
    "editor.maxTokenizationLineLength": 200000,
    "editor.minimap.showSlider": "always",
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.mouseWheelZoom": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.suggestSelection": "first",
    "editor.wordWrapColumn": 100,
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "files.associations": {
        ".pylintrc": "ini",
        ".desktop": "ini",
        ".slurm": "bash",
        ".out": "text",
        "docker-compose.y*ml": "yaml"
    },
    "files.autoSave": "afterDelay",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.idea": true,
        "**/.DS_Store": true,
        "**/*.swp": true,
        "**/*~": true,
        "**/.pytest_cache": true,
        "**/.ruff_cache": true,
        "**/.mypy_cache": true,
        "**/.ipynb_checkpoints": true,
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/.venv": true,
        "**/venv": true,
        "**/.tox": true,
    },
    "files.watcherExclude": {
        "**/__pycache__/**": true,
        "**/.git/**": true,
        "**/.svn/**": true,
        "**/.idea/**": true,
        "**/.DS_Store": true,
        "**/.pytest_cache/**": true,
        "**/.ruff_cache/**": true,
        "**/.mypy_cache/**": true,
        "**/.ipynb_checkpoints/**": true,
        "**/*.pyc": true,
        "**/node_modules/**": true,
        "**/bower_components/**": true,
        "**/build/**": true,
        "**/_build/**": true,
        "**/dist/**": true,
        "**/.eggs/**": true,
        "**/eggs/**": true,
        "**/.venv/**": true,
        "**/venv/**": true,
        "**/.tox/**": true,
        "**/.nox/**": true,
        "**/.direnv/**": true,
        "**/mambaforge/**": true,
        "**/.cache/**": true,
        "**/.hypothesis/**": true,
        "**/tmp/**": true,
        // ML Data
        "data{,/**}": true,
        "datasets{,/**}": true,
        "wandb{,/**}": true,
        "coco{,/**}": true,
        "**/train2014/**": true,
        "**/val2014/**": true,
        "**/train2017/**": true,
        "**/val2017/**": true,
        "**/coco_vanilla/**": true,
        "**/coco/**/*.txt": true,
        "**/coco/annotations/**": true,
        "**/coco/images/**": true,
        "**/coco/labels/**": true,
        "**/data/eval_3.2/**": true,
        "**/data/eval2-samples/**": true,
        "**/data/tests/eval*": true,
        "**/messages/messages/**": true,
        "**/mmcv/tests/**": true,
        "**/semafor/datalake/**": true,
        "**/semafor/sandbox/**": true,
        "**/vanilla/**": true,
        // ML Artifacts & Logs
        "**/checkpoints/**": true,
        "**/experiments/**": true,
        "**/lightning_logs/**": true,
        "**/output/**": true,
        "**/outputs/**": true,
        "**/results/**": true,
        "**/save/**": true,
        "**/weights/**": true,
        "**/work_dirs/**": true,
        "**/*.ckpt": true,
        "**/*.pkl": true,
        "**/*.pt": true
    },
