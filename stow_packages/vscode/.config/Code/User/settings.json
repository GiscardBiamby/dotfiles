    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.extension.enableComposeLanguageServer": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.fastScrollSensitivity": 9,
    "editor.fontFamily": "'Atkinson Hyperlegible Mono', 'Source Code Pro','Droid Sans Mono', 'monospace', monospace, Menlo, Monaco, 'Courier New'",
    "editor.fontLigatures": true,
    "editor.guides.bracketPairs": "active",
    "editor.largeFileOptimizations": false,
    "editor.maxTokenizationLineLength": 200000,
    "editor.minimap.autohide": "mouseover",
    "editor.minimap.showSlider": "always",
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.mouseWheelZoom": true,
    "editor.quickSuggestionsDelay": 100,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.suggestSelection": "first",
    "editor.wordWrapColumn": 100,
    "evenBetterToml.schema.associations": {
        // "pyproject.toml": "https://json.schemastore.org/pyproject.json"
    },
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        ".desktop": "ini",
        ".out": "text",
        ".pylintrc": "ini",
        ".slurm": "bash",
        "docker-compose.y*ml": "yaml"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1500,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.idea": true,
        "**/.ipynb_checkpoints": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/.ruff_cache": true,
        "**/.svn": true,
        "**/.tox": true,
        "**/.venv": true,
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/*.swp": true,
        "**/*~": true,
        "**/*egg-info": true,
        "**/venv": true
    },
    "files.watcherExclude": {
        "**/__pycache__/**": true,
        "**/.DS_Store": true,
        "**/.git/**": true,
        "**/.idea/**": true,
        "**/.mim/**": true,
        "**/.svn/**": true,
        "**/*.egg-info": true,
        "**/.pytest_cache/**": true,
        "**/.ruff_cache/**": true,
        "**/.mypy_cache/**": true,
        "**/.ipynb_checkpoints/**": true,
        "**/*.pyc": true,
        "**/node_modules/**": true,
        "**/bower_components/**": true,
        "**/build/**": true,
        "**/_build/**": true,
        "**/dist/**": true,
        "**/.eggs/**": true,
        "**/eggs/**": true,
        "**/.venv/**": true,
        "**/venv/**": true,
        "**/.tox/**": true,
        "**/.nox/**": true,
        "**/.direnv/**": true,
        "**/mambaforge/**": true,
        "**/.cache/**": true,
        "**/.hypothesis/**": true,
        "**/tmp/**": true,
        // ML Data
        "data{,/**}": true,
        "datasets{,/**}": true,
        "wandb{,/**}": true,
        "coco{,/**}": true,
        "**/train2014/**": true,
        "**/val2014/**": true,
        "**/train2017/**": true,
        "**/val2017/**": true,
        "**/coco_vanilla/**": true,
        "**/coco/**/*.txt": true,
        "**/coco/annotations/**": true,
        "**/coco/images/**": true,
        "**/coco/labels/**": true,
        "**/data/eval_3.2/**": true,
        "**/data/eval2-samples/**": true,
        "**/data/tests/eval*": true,
        "**/messages/messages/**": true,
        "**/mmcv/tests/**": true,
        "**/semafor/datalake/**": true,
        "**/semafor/sandbox/**": true,
        "**/vanilla/**": true,
        // ML Artifacts & Logs
        "**/checkpoints/**": true,
        "**/experiments/**": true,
        "**/lightning_logs/**": true,
        "**/output/**": true,
        "**/outputs/**": true,
        "**/results/**": true,
        "**/save/**": true,
        "**/weights/**": true,
        "**/work_dirs/**": true,
        "**/*.ckpt": true,
        "**/*.pkl": true,
        "**/*.pt": true
